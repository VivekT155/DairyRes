<ThirdWindow>:
    name: 'third'
    MDNavigationLayout:
        ScreenManager:
            Screen:
                BoxLayout:
                    orientation: 'vertical'
                    MDToolbar:
                        title: 'TrueFresh'
                        left_action_items: [["menu", lambda x: nav_drawer.set_state('toggle')]]
                        elevation:10

                    Widget:

                MDCard:
                    size_hint: None, None
                    size: "290dp", "35dp"
                    #md_bg_color: 1,1,0,1
                    pos_hint: {"center_x": .5, "center_y": .8}
                    MDChooseChip:

                        MDChip:
                            text: 'MORNING'
                            text_color: "white"
                            icon: ''
                            selected_chip_color: 186/255, 74/255, 74/255, 1
                            on_press: app.user_selection().morning()

                        MDChip:
                            text: 'EVENING'
                            text_color: "white"
                            icon: ''
                            selected_chip_color: 186/255, 74/255, 74/255, 1
                            on_press: app.user_selection().evening()

                        MDChip:
                            text: 'BOTH'
                            text_color: "white"
                            icon: ''
                            selected_chip_color: 186/255, 74/255, 74/255, 1
                            on_press: app.user_selection().both()

                MDCard:
                    size_hint: None, None
                    size: "241dp", "35dp"
                    #md_bg_color: 1,1,0,1
                    pos_hint: {"center_x": .5, "center_y": .7}
                    MDChooseChip:

                        MDChip:
                            text: 'COW'
                            text_color: "white"
                            icon: ''
                            selected_chip_color: 89/255, 186/255, 74/255, 1
                            on_press: app.user_selection().cow()

                        MDChip:
                            text: 'BUFFALO'
                            text_color: "white"
                            icon: ''
                            selected_chip_color: 89/255, 186/255, 74/255, 1
                            on_press: app.user_selection().buffalo()

                        MDChip:
                            text: 'MIX'
                            text_color: "white"
                            icon: ''
                            selected_chip_color: 89/255, 186/255, 74/255, 1
                            on_press: app.user_selection().mix()

                MDCard:
                    size_hint: None, None
                    size: "221dp", "35dp"
                    #md_bg_color: 1,1,0,1
                    pos_hint: {"center_x": .5, "center_y": .5}
                    MDChooseChip:

                        MDChip:
                            text: 'EVERYDAY'
                            text_color: "white"
                            icon: ''
                            selected_chip_color: 186/255, 74/255, 74/255, 1
                            on_press: app.user_selection().frequency_eve()

                        MDChip:
                            text: 'ONE TIME'
                            text_color: "white"
                            icon: ''
                            selected_chip_color: 186/255, 74/255, 74/255, 1
                            on_press: app.user_selection().frequency_one()

                MDFloatLayout:

                    MDIconButton:
                        id:'date_picker'
                        icon: 'calendar'
                        theme_text_color: "Custom"
                        text_color: app.theme_cls.primary_color
                        pos_hint: {'center_x': .5, 'center_y': .4}
                        user_font_size: 45
                        on_press: app.show_date_picker()

                BoxLayout:
                    spacing: "40dp"
                    size_hint_y: None
                    pos_hint: {'center_x': .5, 'center_y': .2}
                    height: self.minimum_height

                    Widget:

                    MDIconButton:
                        icon: "minus"
                        md_bg_color: app.theme_cls.primary_color
                        user_font_size: "14sp"
                        on_release: label_value.value -= 0.5 if label_value.value > 0 else 0

                    Label:
                        id:label_value
                        value: 1
                        text: str(self.value)
                        size_hint: None, None
                        size: self.texture_size
                        color: 0,0,0,1
                        pos_hint: {"center_y": .5}
                    MDIconButton:
                        icon: "plus"
                        md_bg_color: app.theme_cls.primary_color
                        user_font_size: "14sp"
                        on_release: label_value.value += 0.5

                    Widget

                Label:
                    text: "FREQUENCY"
                    bold: True
                    pos_hint: {"center_x": .5, "center_y": .57}
                    font_size: '20sp'
                    color: 47/255,79/255,79/255,1

                Label:
                    id:meow
                    text: ""
                    bold: False
                    pos_hint: {"center_x": .5, "center_y": .34}
                    font_size: '14sp'
                    color: 47/255,79/255,79/255,1

                Label:
                    text: "QUANTITY"
                    bold: True
                    pos_hint: {"center_x": .5, "center_y": .28}
                    font_size: '21sp'
                    color: 47/255,79/255,79/255,1

                Label:
                    text: "KG"
                    bold: True
                    pos_hint: {"center_x": .5, "center_y": .15}
                    font_size: '16sp'
                    color: 47/255,79/255,79/255,1

                MDFillRoundFlatButton:
                    text: " PLACE  ORDER "
                    md_bg_color: 0,139/255,139/255,1
                    theme_text_color: "Custom"
                    text_color: 1,1,1,1
                    pos_hint: {'center_x': .5, 'center_y': .07}
                    on_press: app.confirm_order() #app.user_selection().print_details()


        MDNavigationDrawer:
            id: nav_drawer
            ContentNavigationDrawer:

