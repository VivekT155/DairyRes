<FirstWindow>:
    name: "first"
    MDFloatLayout:
        Image:
            source: "123.png"
            size_hint_y: None
            height: "240dp"
            pos_hint: {"y": .60}

    MDLabel:
        id: welcome_label
        text: "Welcome To TrueFresh"
        font_name: "BRITANIC.ttf"
        color: app.theme_cls.primary_color
        pos_hint: {"center_x": 0.5,"center_y": 0.20}
        text_color: 1, 1, 0, 1
        font_size: 18
        halign: 'center'
        size_hint_y: None
        height: self.texture_size[1]
        padding_y: 15

    MDTextFieldRound:
        id: phn_login
        hint_text: "Phone Number"
        color_active: 152/255,251/255,152/255,1
        icon_left: "account"
        size_hint_x: None
        width: 150
        font_size: 14
        pos_hint: {"center_x": 0.5,"center_y": 0.6}
    MDTextFieldRound:
        id: password
        hint_text: "OTP"
        color_active: 152/255,251/255,152/255,1
        icon_left: "key"
        size_hint_x: None
        width: 150
        font_size: 14
        pos_hint: {"center_x": 0.5,"center_y": 0.52}
        password: False

    MDRoundFlatButton:
        id: login_otp
        text: "GENERATE OTP"
        font_size: 12
        size_hint_y: 0.05
        pos_hint: {"center_x": 0.5,"center_y": 0.44}
        on_press: app.generate_otp_login()

    MDRoundFlatButton:
        text: "        LOG IN        "
        pos_hint: {"center_x": 0.5,"center_y": 0.36}
        on_release:
            app.root.current = "third"
            #root.manager.transition.direction = "up"

    MDRoundFlatButton:
        text: "SIGN UP"
        font_size: 12
        pos_hint: {"center_x": 0.5,"center_y": 0.28}
        on_release:
            app.root.current = "second"
            root.manager.transition.direction = "left"


